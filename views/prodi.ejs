<h1>Daftar Program Studi di UMDP</h1>
<table border="1">
  <thead>
    <tr>
      <th>No</th>
      <th>Program Studi</th>
      <th>Fakultas</th>
      <th>Singkatan</th>
    </tr>
  </thead>
  <tbody>
    <% prodi.forEach(function(item, index) { %>
    <tr>
      <td><%= index + 1 %></td>
      <td><%= item.prodi %></td>
      <td><%= item.fakultas %></td>
      <td><%= item.singkatan %></td>
    </tr>
    <% }); %>
  </tbody>
</table>
